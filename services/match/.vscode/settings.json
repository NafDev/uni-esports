		"editor.defaultFormatter": "samverschueren.linter-xo"
	},
	"material-icon-theme.activeIconPack": "nest",
	"cSpell.words": ["csgo", "dathost", "esports", "nats", "nestjs", "Ogma", "pino", "supertokens", "UKUE"]
}
