{
  "name": "api",
  "version": "0.0.1",
  "description": "Recipe for NestJS + SuperTokens sessions, with Prisma schema",
  "author": "NafDev",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "nodemon --config",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "db:gen": "prisma generate",
    "db:mig:dev": "dotenv -e development.env \"npx prisma migrate dev\""
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^3.10.0",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "bitfield": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.0",
    "dotenv": "^15.0.0",
    "handlebars": "^4.7.7",
    "mjml": "^4.12.0",
    "nanoid": "^3.2.0",
    "nestjs-pino": "^2.5.0",
    "nodemailer": "^6.7.2",
    "pino-http": "^6.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "steamid": "^2.0.0",
    "supertokens-node": "^8.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/mjml": "^4.7.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/steamid": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.40.0",
    "eslint-config-xo-space": "^0.32.0",
    "eslint-config-xo-typescript": "^0.50.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.3.2",
    "prisma": "^3.10.0",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.3"
  },
  "prisma": {
    "schema": "src/db/prisma/schema.prisma",
    "env": "development.env"
  }
}
