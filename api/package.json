{
	"name": "@uniesports/api",
	"version": "0.0.1",
	"author": "NafDev",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "rm -rf dist && tsc -p tsconfig.build.json",
		"dev": "nodemon --config",
		"test:setup": "docker compose -f test/scripts/docker-compose.yml up -d && sleep 2 && test/scripts/setup-db.sh",
		"test:jest": "concurrently -k -n jest,app -s first --hide app --prefix none \"jest\" \"node -r ts-node/register -r dotenv/config src/main.ts dotenv_config_path=.env.test\"",
		"test:teardown": "docker compose -f test/scripts/docker-compose.yml down",
		"test": "run-s -c test:setup test:jest test:teardown",
		"prod": "node dist/main",
		"lint": "xo --prettier --fix"
	},
	"engines": {
		"node": "^18.12"
	},
	"prisma": {
		"schema": "src/db/prisma/schema.prisma",
		"seed": "npx ts-node src/db/prisma/seeds/seed.ts"
	},
	"dependencies": {
		"@nestjs/common": "^9.0.11",
		"@nestjs/core": "^9.0.11",
		"@nestjs/platform-express": "^9.0.11",
		"@nestjs/throttler": "^3.1.0",
		"@prisma/client": "^4.6.1",
		"@uni-esports/interfaces": "workspace:0.0.1",
		"axios": "^1.1.3",
		"bcrypt": "^5.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"envalid": "^7.3.1",
		"handlebars": "^4.7.7",
		"helmet": "^5.1.0",
		"lodash.capitalize": "^4.2.1",
		"lodash.startcase": "^4.4.0",
		"mjml": "^4.12.0",
		"nanoid": "3.3.4",
		"nestjs-pino": "^3.1.1",
		"nodemailer": "^6.7.5",
		"pino-http": "^8.2.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0",
		"supertokens-node": "^11.0.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.1.1",
		"@nestjs/schematics": "^9.0.1",
		"@nestjs/testing": "^9.0.11",
		"@tsconfig/node18": "^1.0.1",
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.13",
		"@types/he": "^1.1.2",
		"@types/jest": "^28.1.8",
		"@types/lodash.capitalize": "^4.2.7",
		"@types/lodash.startcase": "^4.4.7",
		"@types/mjml": "^4.7.0",
		"@types/node": "^17.0.42",
		"@types/nodemailer": "^6.4.4",
		"@types/supertest": "^2.0.12",
		"concurrently": "^7.3.0",
		"dotenv": "^16.0.1",
		"he": "^1.2.0",
		"jest": "^28.1.3",
		"mailhog": "^4.16.0",
		"nodemon": "^2.0.15",
		"npm-run-all": "^4.1.5",
		"pino-pretty": "^8.0.0",
		"prettier": "^2.3.2",
		"source-map-support": "^0.5.20",
		"supertest": "^6.2.4",
		"ts-jest": "^28.0.8",
